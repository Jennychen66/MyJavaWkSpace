package JavaOperators;
import java.util.*;
class Dog{
	String name;
	String says;
}
public class TestDog {
	public static void main(){
		Dog dog1 = new Dog();
		Dog dog2 = new Dog();
		dog1.name = "spot";
		dog1.says = "Ruff!";
		dog2.name = "scruffy";
		dog2.says = "Wurf!";
		System.out.println(dog1.name);
		System.out.println(dog1.says);
		System.out.println(dog2.name);
		System.out.println(dog2.says);
		
	}

}
